#include<stdio.h>
int main(){	
	int costo, pago;
	printf("Ingrese el costo a Comprar : ");	
	scanf("%i",&costo);
	switch(costo){
		case 1: case 2: case 3:			
			pago = 15;			
			break;
		case 4: case 5: case 6: case 7: case 8:						
			pago = 11;
			break;
		default:
			pago = 10;
	}
	printf("Costo por cada Teclado : %i\n", pago);	
	printf("Total a Pagar : %2.f\n", (float)(pago*cantidad));	
	return 0;
}
